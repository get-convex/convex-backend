{
  "name": "docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "npm run start",
    "start": "rm -r docs/api 2>/dev/null; docusaurus start --port $(detect 3000)",
    "build": "rm -r docs/api 2>/dev/null; docusaurus build && cp _redirects build/",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "clean": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "spellcheck": "cspell \"docs/**/*.md*\"",
    "typecheck": "tsc",
    "test": "cspell \"docs/**/*.md*\" && node test.js"
  },
  "dependencies": {
    "@docusaurus/core": "~3.8.1",
    "@docusaurus/preset-classic": "~3.8.1",
    "@mdx-js/react": "~3.1.0",
    "@signalwire/docusaurus-plugin-llms-txt": "^1.1.0",
    "clsx": "2.1.1",
    "prism-react-renderer": "~2.4.1",
    "raw-loader": "4.0.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "convex-ai-chat": "workspace:*",
    "convex-analytics": "workspace:*",
    "convex": "workspace:*",
    "@docusaurus/theme-common": "~3.8.1",
    "heroicons": "^2.0.16",
    "react-dropdown": "~1.11.0",
    "algoliasearch": "~4.24.0",
    "react-markdown": "~10.1.0",
    "@types/react": "^18.0.0",
    "tailwindcss": "^4.1.5",
    "@tailwindcss/postcss": "^4.1.5",
    "postcss": "^8.4.19",
    "@radix-ui/react-icons": "~1.3.0",
    "react-cookie": "~7.2.2",
    "posthog-js": "~1.249.0",
    "dotenv": "^16.4.7",
    "@docusaurus/plugin-content-docs": "~3.8.1",
    "copy-text-to-clipboard": "^3.2.0"
  },
  "devDependencies": {
    "docusaurus-plugin-typedoc": "npm:@convex-dev/docusaurus-plugin-typedoc@0.19.2",
    "typedoc-plugin-markdown": "npm:@convex-dev/typedoc-plugin-markdown@3.15.6",
    "@docusaurus/module-type-aliases": "~3.8.1",
    "@docusaurus/tsconfig": "~3.8.1",
    "typescript": "~5.0.3",
    "webpack": "^5.99.9",
    "@docusaurus/types": "~3.8.1",
    "cspell": "^6.30.2",
    "detect-port": "^2.1.0",
    "@types/node": "^18.17.0",
    "@docusaurus/theme-classic": "~3.8.1",
    "prettier": "3.5.3",
    "tailwind-merge": "^3.2.0",
    "postcss-nested": "~7.0.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  }
}
