{
  "name": "convex-test-demo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "tsc -p convex/tsconfig.json --noEmit",
    "dev": "convex dev",
    "test": "vitest",
    "test:once": "vitest run",
    "test:debug": "vitest --inspect-brk --no-file-parallelism",
    "test:coverage": "vitest run --coverage --coverage.reporter=text"
  },
  "dependencies": {
    "convex": "workspace:*"
  },
  "devDependencies": {
    "@edge-runtime/vm": "^5.0.0",
    "convex-test": "^0.0.41",
    "globals": "~17.0.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4",
    "vite": "^6.4.1"
  }
}
