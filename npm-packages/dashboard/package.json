    "@testing-library/dom": "~10.4.0",
    "openapi-typescript": "~7.8.0",
    "openapi-typescript-helpers": "~0.0.15",
    "@types/react-sparklines": "~1.7.5",
    "eslint-plugin-boundaries": "~5.3.0"
  }
}
