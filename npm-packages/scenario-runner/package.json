    "random-words": "1.3.0",
    "langchain": "^0.3.0",
    "tiktoken": "^1.0.10",
    "prettier": "3.5.2"
  },
  "devDependencies": {
    "@types/node": "^18.17.0",
    "@types/react": "^18.0.0",
    "@types/ws": "^8.5.13",
    "commander": "^11.1.0",
    "convex": "workspace:*",
    "esbuild": "0.25.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "typescript": "~5.0.3",
    "ws": "8.18.0",
    "eslint": "^8.29.0",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@commander-js/extra-typings": "^11.1.0"
  }
