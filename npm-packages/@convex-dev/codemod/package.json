{
  "name": "@convex-dev/codemod",
  "version": "1.0.0",
  "description": "Automatic codemods to help you migrate your codebase to newer versions of Convex",
  "type": "module",
  "main": "./dist/index.cjs",
  "bin": "./dist/index.cjs",
  "files": [
    "dist"
  ],
  "zshy": {
    "exports": "./src/index.ts",
    "bin": "./src/index.ts"
  },
  "scripts": {
    "build": "node version-check.mjs && zshy && tsc-alias -p tsconfig.json",
    "dev": "zshy && tsc-alias -p tsconfig.json && node ./dist/index.cjs",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "prepack": "node src/scripts/dont-publish-alpha-as-latest.mjs"
  },
  "devDependencies": {
    "@types/node": "^18.17.0",
    "@types/progress": "~2.0.7",
    "@types/semver": "^7.3.13",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4",
    "zshy": "0.7.0"
  },
  "dependencies": {
    "@babel/code-frame": "^7.27.1",
    "@sentry/node": "^7.23.0",
    "@types/babel__code-frame": "^7.0.6",
    "chalk": "5",
    "commander": "^14.0.2",
    "execa": "^9.6.0",
    "jest-diff": "^30.0.3",
    "ora": "^9.0.0",
    "progress": "~2.0.3",
    "semver": "^7.6.0",
    "ts-morph": "^27.0.0",
    "tsc-alias": "~1.8.15"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "module": "./dist/index.js",
  "types": "./dist/index.d.cts",
  "exports": {
    ".": {
      "types": "./dist/index.d.cts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  }
}
