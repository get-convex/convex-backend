{
  "name": "@convex-dev/codemod",
  "version": "0.0.2",
  "description": "Automatic codemods to help you migrate your codebase to newer versions of Convex",
  "type": "module",
  "main": "./dist/index.cjs",
  "bin": {},
  "files": [
    "dist"
  ],
  "zshy": {
    "exports": "src/index.ts",
    "bin": {
      "@convex-dev/codemod": "dist/index.js"
    }
  },
  "scripts": {
    "build": "zshy",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/node": "^18.17.0",
    "@types/progress": "~2.0.7",
    "@types/semver": "^7.3.13",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4",
    "zshy": "0.4.1"
  },
  "dependencies": {
    "@babel/code-frame": "^7.27.1",
    "@sentry/node": "^7.23.0",
    "@types/babel__code-frame": "^7.0.6",
    "chalk": "5",
    "commander": "^11.1.0",
    "execa": "^9.6.0",
    "jest-diff": "^30.0.3",
    "ora": "^8.1.1",
    "progress": "~2.0.3",
    "semver": "^7.6.0",
    "ts-morph": "^26.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "module": "./dist/index.js",
  "types": "./dist/index.d.cts",
  "exports": {
    ".": {
      "types": "./dist/index.d.cts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  }
}
