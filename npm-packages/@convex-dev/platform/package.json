{
  "name": "@convex-dev/platform",
  "version": "0.0.1",
  "private": true,
  "description": "",
  "type": "module",
  "scripts": {
    "build": "npm run generateApiSpec && node version-check.mjs && zshy",
    "prepack": "node src/scripts/dont-publish-alpha-as-latest.mjs",
    "test": "echo 0",
    "typecheck": "tsc --noEmit",
    "generateApiSpec": "npm run generateManagementApiSpec && npm run generateDeploymentApiSpec && npm run generateLogStreamApiSpec",
    "generateManagementApiSpec": "openapi-typescript ./management-openapi.json --output ./src/generatedManagementApi.ts --root-types --root-types-no-schema-prefix",
    "generateDeploymentApiSpec": "openapi-typescript ./deployment-openapi.json --output ./src/generatedDeploymentApi.ts --root-types --root-types-no-schema-prefix",
    "generateLogStreamApiSpec": "openapi-typescript ./log-stream-openapi.json --output ./src/generatedLogStreamApi.ts --root-types --root-types-no-schema-prefix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^18.17.0",
    "typescript": "^5.9.2",
    "openapi-typescript": "~7.8.0",
    "openapi-typescript-helpers": "~0.0.15",
    "tsx": "^4.19.4",
    "zshy": "0.6.0"
  },
  "dependencies": {
    "openapi-fetch": "~0.15.0"
  },
  "zshy": {
    "exports": {
      ".": "./src/index.ts",
      "./deploymentApi": "./src/generatedDeploymentApi.ts",
      "./logStreamApi": "./src/generatedLogStreamApi.ts",
      "./managementApi": "./src/generatedManagementApi.ts"
    }
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.cts",
  "exports": {
    ".": {
      "types": "./dist/index.d.cts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./deploymentApi": {
      "types": "./dist/generatedDeploymentApi.d.cts",
      "import": "./dist/generatedDeploymentApi.js",
      "require": "./dist/generatedDeploymentApi.cjs"
    },
    "./logStreamApi": {
      "types": "./dist/generatedLogStreamApi.d.cts",
      "import": "./dist/generatedLogStreamApi.js",
      "require": "./dist/generatedLogStreamApi.cjs"
    },
    "./managementApi": {
      "types": "./dist/generatedManagementApi.d.cts",
      "import": "./dist/generatedManagementApi.js",
      "require": "./dist/generatedManagementApi.cjs"
    }
  }
}
